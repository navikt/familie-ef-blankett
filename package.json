{
  "name": "famile-ef-blankett",
  "version": "0.1.0",
  "author": "Nav - teamfamilie",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node build/backend/server.js",
    "eject": "react-scripts eject",
    "dev": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,CRA\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\" \"yarn dev:client\"",
    "start:dev": "tsc && NODE_ENV=development node --es-module-specifier-resolution=node --trace-deprecation build/backend/server.js",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "rm -f build/build/server.js && wait-on -l build/backend/server.js && nodemon --es-module-specifier-resolution=node build/backend/server.js --delay 500ms",
    "build": "yarn build:server",
    "build:server": "tsc & NODE_ENV=production webpack --config webpack.config.server.js",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 src/"
    ]
  },
  "dependencies": {
    "@navikt/familie-logging": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.12",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.6",
    "concurrently": "^8.0.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
