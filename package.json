{
  "name": "famile-ef-blankett",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node build/backend/server.js",
    "eject": "react-scripts eject",
    "dev": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,CRA\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\" \"yarn dev:client\"",
    "dev:client": "wait-on -l tcp:8033 && craco start",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/backend/server.js && nodemon build/backend/server.js --delay 500ms",
    "build": "yarn build:server",
    "build:client": "craco build",
    "build:server": "webpack --config webpack.config.server.js",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/"
    ]
  },
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@navikt/familie-backend": "^5.0.31",
    "@navikt/familie-logging": "^1.0.5",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^18.0.10",
    "@types/react-pdf": "^5.7.2",
    "@types/styled-components": "^5.1.26",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "concurrently": "^5.3.0",
    "craco-less": "^1.17.0",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-app": "^6.2.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "html-react-parser": "^0.14.2",
    "husky": "^8.0.3",
    "is-promise": "^4.0.0",
    "lint-staged": "^13.1.0",
    "lodash.throttle": "^4.1.1",
    "mustache-express": "^1.3.2",
    "nav-frontend-chevron-style": "^0.3.9",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "query-string": "^6.13.7",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-pdf": "^5.7.2",
    "react-scripts": "4.0.1",
    "styled-components": "^5.3.6",
    "ts-loader": "^8.0.11",
    "typescript": "^4.9.4",
    "wait-on": "^5.2.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.52",
    "gh-pages": "^4.0.0"
  }
}
